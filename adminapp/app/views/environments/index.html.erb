<p id="notice"><%= notice %></p>

<h1>Environments</h1>

<table>
  <thead>
    <tr>
      <th>Nam </th>
      <th>Environment</th>
      <th>Desc </th>
      <th>Environment</th>
      <th>Id multimedia</th>
      <th>Id user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @environments.each do |environment| %>
      <tr>
        <td><%= environment.nam_ %></td>
        <td><%= environment.environment %></td>
        <td><%= environment.desc_ %></td>
        <td><%= environment.environment %></td>
        <td><%= environment.id_multimedia %></td>
        <td><%= environment.id_user %></td>
        <td><%= link_to 'Show', environment %></td>
        <td><%= link_to 'Edit', edit_environment_path(environment) %></td>
        <td><%= link_to 'Destroy', environment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Environment', new_environment_path %>
